[ctns_demo rwu]

[ctns_make id='num-imagineQ1']

    [ctns_script]
        symbols.exp = imaginary();

        q.push(tex({'symbols':symbols, 'reveal':'hide', 'system':'katex'},'
$$ ${exp} $$ is an imaginary number.
')); 
    
        r.push(tex({'symbols':symbols, 'reveal':'hide', 'system':'katex'},'
The set of imaginary numbers is the set of all products $$bi$$ with $$b \in \mathbb{R}$$ and $$i^2=-1$$, $|\{bi\,|\,b \in \mathbb{R}, i^2=-1\}|$
')); 

        $(qblock).html(q.join(' '));
        $(rightblock).html(r.join(' '));
        $(wrongblock).html(r.join(' '));
        $(undecidedblock).html(r.join(' '));
    [/ctns_script]

[/ctns_make]

[ctns_make id='num-imagineQ2']

    [ctns_script]
        symbols.exp = natural();

        q.push(tex({'symbols':symbols, 'reveal':'hide', 'system':'katex'},'
$$ ${exp} $$ is an imaginary number.
')); 
    
        r.push(tex({'symbols':symbols, 'reveal':'hide', 'system':'katex'},'
The set of imaginary numbers is the set of all products $$bi$$ with $$b \in \mathbb{R}$$ and $$i^2=-1$$, $|\{bi\,|\,b \in \mathbb{R}, i^2=-1\}|$
')); 

        $(qblock).html(q.join(' '));
        $(rightblock).html(r.join(' '));
        $(wrongblock).html(r.join(' '));
        $(undecidedblock).html(r.join(' '));
    [/ctns_script]

    [ctns_element type='build_a_graph' swap_a_b='true'][/ctns_element]

[/ctns_make]

[ctns_make id='num-wholeQ1']

    [ctns_script]
        symbols.exp = whole();

        q.push(tex({'symbols':symbols, 'reveal':'hide', 'system':'katex'},'
$$ ${exp} $$ is a whole number.
')); 
    
        r.push(tex({'symbols':symbols, 'reveal':'hide', 'system':'katex'},'
The set of whole numbers $$\mathbb{W}$$ is the combined set of zero $$0$$ and the natural numbers, $|\mathbb{W} = \{0, 1, 2, ... \}.|$
')); 

        $(qblock).html(q.join(' '));
        $(rightblock).html(r.join(' '));
        $(wrongblock).html(r.join(' '));
        $(undecidedblock).html(r.join(' '));
    [/ctns_script]

