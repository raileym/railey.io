[text coordinates='(-3,10)'][gmath precision='2' points='a,h,k']f(x)=a.v(x-h.v)^2+k.v[/gmath][/text]
[/panel]
[function color='blue' variable='x' constants='a,h,k' min='-20' max='20']a.pv*pow(x-h.pv,2)+k.pv[/function]
