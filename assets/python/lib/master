[point id='D' points='A' type='match' ycoord='-3' labeloffsetzero='true' showpoint='false' showlabel='false' showcoordinates='false' animate='false']
[point id='E' points='B' type='match' ycoord='-3' labeloffsetzero='true' showpoint='false' showlabel='false' showcoordinates='false' animate='false']
[point id='F' points='D,E' type='inline' distance='0.5' labeloffsetzero='true' showpoint='false' showlabel='false' showcoordinates='false' animate='false']
[segment id='DE' start='D' end='E' color='blue' showarrowstart='true' showarrowend='true' arrowtype='arrowbar']

[calculate id='C1' points='A,B' operation='abs(A.x-B.x)']
[calculate id='C2' points='A,B' operation='(B.x - A.x) GE 0']
[calculate id='C3' points='A,B' operation='B.x - A.x']

[text point='A' offsetx='-0.5' offsety='2'][gmath]a[/gmath][/text]
[text point='B' offsetx='-0.5' offsety='2'][gmath]b[/gmath][/text]
[text id='T3' point='F' offsetx='-1.5' offsety='-1.5'][gmath]|b-a|[/gmath][/text]

[condition onTrue='C2' show='T1' hide='T2']

[panel control='graph' height='160']
[text id='T1' fontsize='14' align='center' coordinates='(0,9)']
[gmath addparen='true' points='A,B,C1']
/begin{array}{rcl}
|b-a| & = & |B.x-A.x| ////
& = & |C1.x| ////
& = & C1.x
/end{array}
[/gmath][/text]
[text id='T2' fontsize='14' align='center' coordinates='(0,9)']
[gmath addparen='true' points='A,B,C1,C3']
/begin{array}{rcl}
|b-a| & = & |B.x-A.x| ////
& = & |C3.nx| ////
& = & -(C3.nx)////
& = & C1.x
/end{array}
[/gmath][/text]
[/panel]

[/graph]
[/beciteable_citation]
EOT;

