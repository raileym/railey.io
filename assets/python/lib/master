<?php
class Tml_Linear_Graph_Q_Master extends Tml_Question
{
    private $integer_very_small_nonzero;

    function __construct($key, $integer_very_small_nonzero) {
        parent::__construct($key);
        $this->integer_very_small_nonzero = $integer_very_small_nonzero;
    }
    
    function init() {

        $m = $this->integer_very_small_nonzero->next();
        $b = $this->integer_very_small_nonzero->next();
        
        $this->question = '';
        $this->wp_script = '';
        
		$graph = new Build_A_Graph(array('HEIGHT'=>'600', 'TAG'=>'A'));
        
        $this->question .= <<< EOT
{$graph->divBlock}
<hr class='pm-struct-default' />
EOT;

        $this->wp_script .= <<< EOT
[hr]
[graph name='{$graph->BASE}' yaxislabel='f(x)' title='The Identity Function' height='300' width='300']
[panel height='120' boundingbox='-10,8,10,-1']
[slider id='b' start='(-8,0)' end='(5,0)' linecolor='blue' step='1' initial='{$b}']
[slider id='m' start='(-8,2)' end='(5,2)' linecolor='green' step='1' initial='{$m}']
[text align='center' coordinates='(-3,7)'][gmath points='m,b']f(x)=m.pv x + b.pv[/gmath][/text]
[/panel]
[function color='black' variable='x' constants='m,b' min='-20' max='20']m.pv*x+b.pv[/function]
[/graph]
EOT;

		$graph = new Build_A_Graph(array('HEIGHT'=>'600', 'TAG'=>'B'));
        
        $this->question .= <<< EOT
{$graph->divBlock}
<hr class='pm-struct-default' />
EOT;

