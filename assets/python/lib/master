[text coordinates='(-3,7)'][gmath points='c']f(x)=c.v[/gmath][/text]
[/panel]
[function color='blue' variable='x' constants='c' min='-20' max='20']c.pv[/function]
[/graph]
EOT;

		$graph = new Build_A_Graph(array('HEIGHT'=>'600', 'TAG'=>'C'));
        
        $this->question .= <<< EOT
{$graph->divBlock}
