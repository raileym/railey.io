/begin{array}{rcl}
|b-a| & = & |B.x-A.x| ////
& = & |C1.x| ////
& = & C1.x ////
/end{array}
[/gmath][/text]
[text id='T2' fontsize='14' align='center' coordinates='(0,9)']
[gmath addparen='true' points='A,B,C1,C3']
/begin{array}{rcl}
|b-a| & = & |B.x-A.x| ////
& = & |C3.nx| ////
& = & -(C3.nx) ////
& = & C1.x ////
/end{array}
[/gmath][/text]
[/panel]

[/graph]
[/beciteable_citation]
